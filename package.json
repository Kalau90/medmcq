{
  "name": "au-medicin-mcq",
  "version": "0.2.1",
  "description": "En webapp til visning af MCQ-opgaver fra kandidaten i medicin p√• AU",
  "main": "server.js",
  "engines": {
    "node": "10.8.0",
    "npm": "6.2.0"
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "none",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "arrowParens": "always"
  },
  "repository": "https://github.com/Morsby/au-medicin-mcq",
  "scripts": {
    "format": "prettier-eslint \"client/src/**/*.js\" --write",
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "nodemon server.js",
    "client": "npm run genversion && cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "start": "node server.js",
    "heroku-postbuild": "npm install --dev --prefix client && npm run build --prefix client",
    "update": "git pull && npm install && cd client && npm install",
    "build": "npm run update && npm run genversion && cd client && npm run build",
    "genversion": "genversion -e version.js && mv version.js client/src/"
  },
  "author": "Morsby",
  "license": "MIT",
  "dependencies": {
    "@sendgrid/mail": "^6.3.1",
    "async": "^2.6.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cloudinary": "^1.13.2",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "dotenv": "^6.2.0",
    "eslint": "^5.12.0",
    "express": "^4.16.4",
    "helmet": "^3.15.0",
    "heroku-ssl-redirect": "0.0.4",
    "json2csv": "^4.3.5",
    "keygrip": "^1.0.3",
    "lodash": "^4.17.11",
    "mongoose": "^5.4.4",
    "mongoose-simple-random": "^0.4.1",
    "mongoose-unique-validator": "^2.0.2",
    "multer": "^1.4.1",
    "multer-storage-cloudinary": "^2.2.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "randomstring": "^1.1.5",
    "sanitize-html": "^1.20.0"
  },
  "devDependencies": {
    "concurrently": "^3.6.1",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "genversion": "^2.1.1",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.0",
    "prettier-eslint-cli": "^4.7.1"
  }
}
